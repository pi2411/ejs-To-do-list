<%- include('header'); -%>

  <div class="box" id="heading">
    <h1> <%=listTitle%></h1>
  </div>

  <div class="box">
    <% for(var i=0;i<newListItem.length;i++){ %>
      <div class="item">
        <input type="checkbox">
          <p><%= newListItem[i] %></p>
      </div>
    <% } %>
    <form class="item" action="/"  method="post">
       <input id ="Input" type="text" name="Item" placeholder="Item" autocomplete="off" >
       <button type="submit" name="listAdd" value=<%=listTitle%>>+</button>
       </form>

       <form class="item" action="/delte" method="post">
         <input id ="Input" type="text" name="Item" placeholder="Delte Item" autocomplete="off" >
          <button type="submit" name="listDelte" value=<%=listTitle%>>+</button>
          </form>


  </div>
<%- include('footer'); -%>
